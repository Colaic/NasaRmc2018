<launch>
    <node name="drivebase_odom_publisher" pkg="tfr_sensor" type="drivebase_odom_publisher" output="screen">
        <rosparam>
            parent_frame: odom
            child_frame: base_footprint
            wheel_span: 55.88
        </rosparam>
        <remap from="/drivebase_odometry" to="/sensors/drivebase_odom"/>
    </node>
</launch>
